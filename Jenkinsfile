pipeline {
    agent any

    stages {
        stage('Jenkins Hello') {
            steps {
                echo 'Hello World'
                echo 'Trying another test...'
            }
        }
        
       stage('Jenkins Goodbye') {
            steps {
                echo 'Goodbye World'
                echo 'Trying another another test...'
            }
        }
       
       stage('Jenkins Hello Again') {
            steps {
                echo 'Hello Again'
                echo 'Trying another another ANOTHER test...'
            }
        }
        
       stage('Jenkins Goodbye Again') {
            steps {
                echo 'Hello Again'
                echo 'Trying another another ANOTHER test...'
            }
        }
    }
}
